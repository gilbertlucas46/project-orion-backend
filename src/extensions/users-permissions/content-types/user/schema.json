{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "proof_of_identity": {
      "type": "component",
      "repeatable": false,
      "component": "user-info.proof-of-identity"
    },
    "account_status": {
      "type": "enumeration",
      "enum": [
        "awaiting_approval",
        "declined",
        "approved"
      ],
      "required": true,
      "default": "awaiting_approval"
    },
    "proof_of_address": {
      "type": "component",
      "repeatable": false,
      "component": "user-info.utility-bill"
    },
    "user_type": {
      "type": "enumeration",
      "enum": [
        "customer",
        "service_professional"
      ]
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "users_permissions_user"
    }
  }
}
